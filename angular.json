{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "app_one": {
      "projectType": "application",
      "schematics": {},
      "root": "projects/app_one",
      "sourceRoot": "projects/app_one/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-architects/native-federation:build",
          "options": {},
          "configurations": {
            "production": {
              "target": "app_one:esbuild:production"
            },
            "development": {
              "target": "app_one:esbuild:development",
              "dev": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-architects/native-federation:build",
          "options": {
            "target": "app_one:serve-original:development",
            "rebuildDelay": 0,
            "dev": true,
            "port": 0
          }
        },
        "extract-i18n": {
          "builder": "@angular/build:extract-i18n"
        },
        "test": {
          "builder": "@angular/build:karma",
          "options": {
            "polyfills": ["zone.js", "zone.js/testing"],
            "tsConfig": "projects/app_one/tsconfig.spec.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app_one/public"
              }
            ],
            "styles": ["projects/app_one/src/styles.css"]
          }
        },
        "esbuild": {
          "builder": "@angular/build:application",
          "options": {
            "browser": "projects/app_one/src/main.ts",
            "polyfills": ["zone.js", "es-module-shims"],
            "tsConfig": "projects/app_one/tsconfig.app.json",
            "assets": [
              {
                "glob": "**/*",
                "input": "projects/app_one/public"
              }
            ],
            "styles": ["projects/app_one/src/styles.css"]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kB",
                  "maximumError": "1MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4kB",
                  "maximumError": "8kB"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-original": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "app_one:esbuild:production"
            },
            "development": {
              "buildTarget": "app_one:esbuild:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201
          }
        }
      }
    },
